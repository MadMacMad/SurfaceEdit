#pragma kernel CSMain

RWTexture2D<float4> Result;
float2 TextureSize;

float4 TintColor;

int2 RenderOrigin;
int2 RenderSize;

[numthreads(32, 32, 1)]
void CSMain(uint3 id : SV_DispatchThreadID)
{
	id = uint3(id.x + RenderOrigin.x, id.y + RenderOrigin.y, 0);

	Result[id.xy] = Result.Load(id) * TintColor;
}

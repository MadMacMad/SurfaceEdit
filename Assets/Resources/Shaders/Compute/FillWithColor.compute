#pragma kernel CSMain

RWTexture2D<float4> Result;
float2 TextureSize;

int2 RenderOrigin;
int2 RenderSize;

float4 Color;

[numthreads(32,32,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
	if (id.x > RenderSize.x || id.y > RenderSize.y)
		return;

	int2 newID = int2(id.x + RenderOrigin.x, id.y + RenderOrigin.y);

	Result[newID] = Color;
}
